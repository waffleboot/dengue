++(NSMutableURLRequest*) parseTextData:(NSArray*)textData forSmsNumber:(SmsNumber*)smsNumber {
+    DEBUG_REQUEST_KIND(RequestKindParseTextData);
+    NSMutableString *body = [NSMutableString string];
+    NSString *list = [NSString stringWithString:[self requiredListOfStrings:textData]];
+    [body appendFormat:@"<defPlaceFromId xsi:type=\"xsd:string\">%@</defPlaceFromId>\n", [smsNumber.placeId stringValue]];
+    [body appendFormat:@"<defCatId xsi:type=\"xsd:string\">%@</defCatId>\n", [smsNumber.categoryId stringValue]];
+    [body appendFormat:@"<defSrcId xsi:type=\"xsd:string\">%@</defSrcId>\n", [smsNumber.sourceId stringValue]];
+    [body appendFormat:@"<defPlaceToId xsi:type=\"xsd:string\">%@</defPlaceToId>\n", [smsNumber.placeToId stringValue]];
+    [body appendString:list];
+    NSString *SOAPMessageCore = [self createSOAPMessageCore:@"parseTextData" messageCore:body];
+    return [self makeRequestWithSOAPCore:SOAPMessageCore];
+}
+
