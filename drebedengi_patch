+- (void)executePasteboardSynchronization {
+  if (!gUserId) return;
+  NSString *pasteboardText = [UIPasteboard generalPasteboard].string;
+  if (pasteboardText && pasteboardText.length > 0) {
+    [self synchronizePasteboardText:pasteboardText];
+  }
+}
+
+- (void)synchronizePasteboardText:(NSString*)text {
+  NSString *smsText = text.smsText;
+  if (smsText.length == 0 || smsText.length > 512) {
+    return;
+  }
+  if (!self.latestSmsText) {
+    self.latestSmsText = [[self currectUserStateDict] valueForKey:@"latestSmsText"];
+    if (!self.latestSmsText) {
+      self.latestSmsText = @"";
+    }
+  }
+  if ([self.latestSmsText isEqualToString:smsText]) {
+    return;
+  }
+  NSDate *date = [NSDate date];
+  self.latestSmsText = smsText;
+  [self userStateSetValue:smsText forKey:@"latestSmsText"];
+  dispatch_async(dbqueue, ^{
+      SmsHistoryStatus status = self.userAccessEnabled ? NEW : NOT_PREMIUM;
+      SmsSynchronization *smsSynchronization = [[SmsSynchronization alloc] initWithDbManager:dbManager];
+      [smsSynchronization addSmsText:smsText forUser:dbManager.currentUser andDate:date andStatus:status];
+  });
+}
 
 @end
 
