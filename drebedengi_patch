+#ifdef DEBUG
+    if (debugRequestKind == RequestKindParseTextData || debugRequestKind == RequestKindSetRecordList) {
+        NSLog(@"\n------------------------------------------------------------------");
+        NSLog(@"\ncurl --data-ascii '%@' https://www.drebedengi.ru/soap/", mes);
+    }
+    debugRequestKind = RequestKindNone;
+#endif
+    
     return req;
 }
 
