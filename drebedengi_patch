@@ -2840,6 +2871,20 @@ -(BOOL) userIsBalanceCorrected {
 
 
 
+-(NSArray*) getNewSmsHistoryItems {
+    NSSortDescriptor *sort = [NSSortDescriptor sortDescriptorWithKey:@"date" ascending:YES];
+#ifdef DEBUG
+    NSArray *items = [self getEntities:@"SmsHistory"];
+    items = [items sortedArrayUsingDescriptors:[NSArray arrayWithObject:sort]];
+    for (SmsHistory *histItem in items) {
+        NSLog(@"id=%@ text='%@' status=%@ date=%@", histItem.moId, histItem.smsText, histItem.status, histItem.date);
+    }
+#endif
+    NSPredicate *predicate = [NSPredicate predicateWithFormat:@"(status == %@ OR status == %@) AND user == %@",
+                              @(NEW), @(NOT_PREMIUM), self.currentUser];
+    NSArray *histItems = [self getEntities:@"SmsHistory" byPredicate:predicate];
+    return [histItems sortedArrayUsingDescriptors:[NSArray arrayWithObject:sort]];
+}
 
