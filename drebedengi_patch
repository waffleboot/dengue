diff --git a/Classes/SmsSynchronization.m b/Classes/SmsSynchronization.m
new file mode 100644
index 0000000..6d29e47
--- /dev/null
+++ b/Classes/SmsSynchronization.m
@@ -0,0 +1,197 @@
+
+#import "SmsSynchronization.h"
+#import "UICoreDataStorage.h"
+
+@interface SmsSynchronization ()
+@property (strong,nonatomic) CoreDataManage *dbManager;
+@end
+
+@implementation SmsSynchronization
+
+- (instancetype)initWithDbManager:(CoreDataManage *)dbManager {
+    if (self = [super init]) {
+        _dbManager = dbManager;
+    }
+    return self;
+}
+
++ (SmsNumber*)smsNumber {
+    NSNumber *defaultWallet = [[UICoreDataStorage sharedUICDStorage] getDefaultWallet].moId;
+    NSNumber *secondWallet  = nil;
+    for (MyObject *obj in [[UICoreDataStorage sharedUICDStorage] getAllRepositoriesWithHiden:NO withDuty:NO]) {
+        if (![obj.moId isEqualToNumber:defaultWallet]) {
+            secondWallet = obj.moId;
+            break;
+        }
+    }
+    NSNumber *sourceId = nil;
+    for (MyObject *obj in [[UICoreDataStorage sharedUICDStorage] getSourceTreeWithHiden:NO withDuty:NO]) {
+        sourceId = obj.moId;
+        break;
+    }
+    SmsNumber *smsNumber = [[[SmsNumber alloc] init] autorelease];
+    smsNumber.placeId    = defaultWallet;
+    smsNumber.categoryId = [[UICoreDataStorage sharedUICDStorage] getDefaultCategory].moId;
+    smsNumber.sourceId   = sourceId;
+    smsNumber.placeToId  = secondWallet;
+    smsNumber.saveSmsToComment = @(YES);
+    smsNumber.timezoneOffset   = @(3);
+    return smsNumber;
+}
+
+- (SOAPRequestDataProvider*)getDataProvider:(id<DataProviderDelegate>)delegate {
+    SOAPRequestDataProvider *provider = nil;
+    NSArray *histItems = [self.dbManager getNewSmsHistoryItems];
+    if (histItems && histItems.count > 0) {
+        NSMutableArray *list = [NSMutableArray arrayWithCapacity:histItems.count];
+        for (SmsHistory *histItem in histItems) {
+            [list addObject:histItem.smsText];
+        }
+        provider = [[SOAPRequestDataProvider alloc] initWithDelegate:delegate];
+        [provider requestParseTextData:list forSmsNumber:[SmsSynchronization smsNumber]];
+    }
+    return provider;
+}
+
++ (void)markNewItemsAsNotPremium {
+//  RLMRealm *realm = [RLMRealm defaultRealm];
+//  [realm beginWriteTransaction];
+//  for (NSNumber<RLMInt> *x in self.smsHistoryIds) {
+//    [SmsHistory objectForPrimaryKey:x].status = @(NOT_PREMIUM);
+//  }
+//  [realm commitWriteTransaction];
+//  self.smsHistoryIds = nil;
+}
+
+- (void)addSmsText:(NSString*)smsText forUser:(User*)user andDate:(NSDate*)date andStatus:(SmsHistoryStatus)status {
+    SmsHistory *histItem = (SmsHistory*) [self.dbManager getNewManagedObjectForName:@"SmsHistory"];
+    histItem.moId    = @([self.dbManager getMaxIdObjectForName:@"SmsHistory"]);
+    histItem.date    = date;
+    histItem.user    = user;
+    histItem.status  = @(status);
+    histItem.smsText = smsText;
+    [self.dbManager saveData];
+}
+
+- (void)processParseTextDataReturn:(NSArray*)list {
+    NSArray *histItems = [self.dbManager getNewSmsHistoryItems];
+    SmsNumber *smsNumber = [SmsSynchronization smsNumber];
+    NSMutableArray<Record*> *records = [NSMutableArray arrayWithCapacity:list.count];
+    for (int i=0; i < MIN(list.count, histItems.count); ++i) {
+        for (Record *record in [self makeRecord:histItems[i]
+                                   forSmsNumber:smsNumber
+                                           from:list[i]]) {
+            [records addObject:record];
+        }
+    }
+    [self.dbManager setListOfRecords:records];
+}
+
+- (NSArray<Record*>*)makeRecord:(SmsHistory*)histItem
+                   forSmsNumber:(SmsNumber*)smsNumber
+                           from:(NSDictionary*)response {
+    
+    if ([response isKindOfClass:[NSNull class]]) {
+        histItem.status = @(NOT_RECOGNIZED);
+    } else if ([response isKindOfClass:[NSDictionary class]]) {
+        
+        NSDictionary *item = (NSDictionary*)response;
+
+        int type = [item[@"type"] intValue];
+        
+        if (type == 0) {
+            histItem.status = @(NOT_RECOGNIZED);
+        } else if (type == 1) {
+            histItem.status = @(IGNORE_SERVER);
+        } else {
+            
+            int curId = [item[@"cur"] intValue];
+
+            if (![self.dbManager existentCurrencyByID:curId]) {
+                return nil;
+            }
+            
+            NSString *comment = item[@"comment"]; if (!comment) comment = @"";
+            if ([item[@"is_autodetected"] boolValue] &&
+                [smsNumber.saveSmsToComment boolValue] &&
+                ![comment containsString:histItem.smsText]) {
+                comment = [NSString stringWithFormat:@"%@ %@", comment, histItem.smsText];
+            }
+            
+            NSDate *operationDate = nil;
+            if (![item[@"is_defaultdate"] boolValue]) {
+                operationDate = [Record getDateFromString:item[@"date"]];
+                operationDate = [operationDate dateByAddingTimeInterval:smsNumber.timezoneOffset.intValue];
+            } else {
+                operationDate = histItem.date;
+            }
+            
+            double summa = [item[@"sum"] doubleValue] / 100;
+            
+            if ([item[@"is_autodetected"] boolValue]) {
+                histItem.status = @(RECOGNIZED_AUTO);
+            } else {
+                histItem.status = @(RECOGNIZED_RULE);
+            }
+            
+            if (type == 4) {
+                int src_repository_id = [item[@"place_from_id"] intValue];
+                int dst_repository_id = [item[@"place_to_id"] intValue];
+                Record* recordFrom = [self makeRecord:-summa
+                                          currency_id:curId
+                                                 date:operationDate
+                                        repository_id:src_repository_id
+                                          category_id:dst_repository_id
+                                       operation_type:type
+                                              comment:comment];
+                Record* recordTo = [self makeRecord:summa
+                                        currency_id:curId
+                                               date:operationDate
+                                      repository_id:dst_repository_id
+                                        category_id:src_repository_id
+                                     operation_type:type
+                                            comment:comment];
+                recordFrom.moveId = recordTo.moId;
+                recordTo.moveId = recordFrom.moId;
+                return @[recordFrom,recordTo];
+            } else {
+                return @[[self makeRecord:summa
+                              currency_id:curId
+                                     date:operationDate
+                            repository_id:[item[@"place_from_id"] intValue]
+                              category_id:[item[@"cat_id"] intValue]
+                           operation_type:type
+                                  comment:comment]];
+            }
+        }
+    }
+    return nil;
+}
+
+- (Record*)makeRecord:(double)summa
+          currency_id:(int)currency_id
+                 date:(NSDate*)date
+        repository_id:(int)repository_id
+          category_id:(int)category_id
+       operation_type:(int)operation_type
+              comment:(NSString *)comment {
+    int recordId = [self.dbManager getMaxIdObjectForName:@"Record"];
+    Record *record = [self.dbManager getOrCreateRecord:recordId];
+    record.objectId = @(category_id);
+    if (comment != nil) {
+        record.comment = comment;
+    } else {
+        record.comment = @"";
+    }
+    record.moId = @(recordId);
+    record.currencyID = @(currency_id);
+    record.isDuty = category_id == -1 ? @(YES) : @(NO);
+    record.isCurrencyChange = @(NO);
+    record.accountId  = @(repository_id);
+    record.difference = @(summa);
+    record.operationType = @(operation_type);
+    record.operationDate = date;
+    return record;
+}
+
+@end
