+  int new_client_id;
+  
+  @synchronized (PRJ_mutexLockIDNumber)
+  {
+    //get last client_id from user defaults, or initialize it
+    NSUserDefaults * standDefaults = [NSUserDefaults standardUserDefaults];
+    //    int new_client_id;
+    
+    if ([standDefaults objectForKey:@"last_client_id"]) {
+      new_client_id = [standDefaults integerForKey:@"last_client_id"];
+    }
+    else {
+      new_client_id = 0;
+    }
+    
+    //increase client_id, check to be unique - iterate id while there is not record with same id
+    new_client_id++;
+    
+      while ([MaxObjectId getObjectForName:entityName inManagedObjectContext:context byId:new_client_id]) {
+      new_client_id++;
+    }
+    
+    //write new id to defaults
+    [standDefaults setInteger:new_client_id forKey:@"last_client_id"];
+    [standDefaults synchronize];
+    
+    //return value
+  }
+  
+  return new_client_id;
+}
+
++ (NSManagedObject *)getObjectForName:(NSString *)entityName
+               inManagedObjectContext:(NSManagedObjectContext*)context
+                                 byId:(long long)Id {
+  
+  NSManagedObject * object;
+  NSEntityDescription * entityDescription = [NSEntityDescription entityForName:entityName inManagedObjectContext:context];
+  
+  NSFetchRequest *request = [[[NSFetchRequest alloc] init] autorelease];
+  [request setEntity:entityDescription];
+  [request setPredicate:[NSPredicate predicateWithFormat:@"moId = %lld",Id]];
+  
+  NSError *error = nil;
+  NSArray * objects = [NSMutableArray arrayWithArray:[context executeFetchRequest:request error:&error]];
+  
+  //    [request release];
+  if (!objects || [objects count] == 0) {
+    object = nil;
+  }
+  else {
+    object = [objects objectAtIndex:0];
+    if ([objects count] > 1)
+    {
+      //DDLogInfo(@"Duplicate object with id : %i", Id);
+    }
+  }
+  return object;
+}
+
+
+@end
