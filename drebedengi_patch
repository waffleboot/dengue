++(NSString*) requiredListOfStrings: (NSArray*) array {
+  NSString *format = @"<list SOAP-ENC:arrayType=\"xsd:string[%d]\" xsi:type=\"SOAP-ENC:Array\">\n";
+  NSMutableString * list = [NSMutableString stringWithFormat: format, array.count];
+  for (NSString *elem in array) {
+    [list appendFormat: @"<item xsi:type=\"xsd:string\">%@</item>\n", elem];
+  }
+  [list appendString: @"</list>\n"];
+  return list;
+}
+
 
 
 
