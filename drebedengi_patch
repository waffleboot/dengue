diff --git a/Classes/Sms/NSString+SMS.m b/Classes/Sms/NSString+SMS.m
new file mode 100644
index 0000000..5c95243
--- /dev/null
+++ b/Classes/Sms/NSString+SMS.m
@@ -0,0 +1,17 @@
+
+#import "NSString+SMS.h"
+
+@implementation NSString (SMS)
+- (NSString*)smsText {
+  NSScanner *scanner   = [NSScanner scannerWithString:self];
+  NSMutableString *acc = [[NSMutableString alloc] initWithCapacity:self.length];
+  NSCharacterSet *doNotWant = [NSCharacterSet characterSetWithCharactersInString:@"\n\r\t "];
+  while (!scanner.atEnd) {
+    NSString *tmp = nil;
+    [scanner scanUpToCharactersFromSet:doNotWant intoString:&tmp];
+    [acc appendString:tmp];
+    [acc appendString:@" "];
+  }
+  return [acc stringByTrimmingCharactersInSet:[NSCharacterSet whitespaceCharacterSet]];
+}
+@end
