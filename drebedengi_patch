+
+-(void) setListOfRecords:(NSArray<Record*>*)list {
+    __weak CoreDataManage *weakSelf = self;
+  [self setRecordListBlock:^Record *(long i) {
+      Record *record = list[i];
+      ChangeListCD *clist = (ChangeListCD*)[weakSelf getNewManagedObjectForName:@"ChangeListCD"];
+      if (record.isCurrencyChange.boolValue) {
+          clist.typeChanged = @"change";
+          clist.actionId = @(2);
+      } else {
+          if (record.operationType.intValue == 4) {
+              clist.typeChanged = @"move";
+          } else {
+              clist.typeChanged = @"waste";
+          }
+          clist.actionId = @(1);
+      }
+      clist.objectId = record.moId;
+      clist.date = [NSDate date];
+      clist.objectTypeId = [NSNumber numberWithInt:1];
+      clist.userId = [NSNumber numberWithLongLong:gIntUserId];
+      return record;
+  } count:list.count];
+}
+
+-(Record*)getOrCreateRecord:(NSInteger)internalID {
+    Record *record = nil;
+    // to accelerate we create Record here
+    NSPredicate *predicate = [NSPredicate predicateWithFormat:@"moId == %d", internalID];
+    NSArray *entities = [self getEntities:@"Record" byPredicate:predicate];
     
+    if (entities) {
+        record =  [entities objectAtIndex:0];
+        [self changeBalanceNotSaveForRecord:record add:NO];
+    }
+    else {
+        record = (Record*)[self getNewManagedObjectForName:@"Record"];
+        record.moId = [NSNumber numberWithInteger:internalID];
+        record.user = _currentUser;
+    }
+    return record;
+}
