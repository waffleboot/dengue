+    //    {
+    //        dispatch_sync(storageQueue, block);
+    //    }
+    
+    //write new id to defaults
+    [standDefaults setInteger:new_client_id forKey:@"last_client_id"];
+    [standDefaults synchronize];
+  }
+  
+  //return value
+  return new_client_id;
+}
